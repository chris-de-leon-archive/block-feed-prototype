{
  "name": "block-gateway-producer",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/block-gateway/producer/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "target": "node",
        "compiler": "tsc",
        "outputPath": "dist/apps/block-gateway/producer",
        "main": "apps/block-gateway/producer/src/main.ts",
        "tsConfig": "apps/block-gateway/producer/tsconfig.app.json",
        "assets": ["apps/block-gateway/producer/src/assets"],
        "isolatedConfig": true,
        "webpackConfig": "apps/block-gateway/producer/webpack.config.js",
        "generatePackageJson": true
      },
      "configurations": {
        "development": {},
        "production": {}
      }
    },
    "serve": {
      "executor": "@nx/js:node",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "block-gateway-producer:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "block-gateway-producer:build:development"
        },
        "production": {
          "buildTarget": "block-gateway-producer:build:production"
        }
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/block-gateway/producer/**/*.ts"]
      }
    }
  },
  "tags": []
}
