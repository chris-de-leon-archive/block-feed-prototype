{
  "name": "api-relayers-update",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/api/relayers/update/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "target": "node",
        "compiler": "tsc",
        "outputPath": "dist/apps/api/relayers/update",
        "main": "apps/api/relayers/update/src/main.ts",
        "tsConfig": "apps/api/relayers/update/tsconfig.app.json",
        "assets": ["apps/api/relayers/update/src/assets"],
        "isolatedConfig": true,
        "webpackConfig": "apps/api/relayers/update/webpack.config.js"
      },
      "configurations": {
        "development": {},
        "production": {}
      }
    },
    "serve": {
      "executor": "@nx/js:node",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "api-relayers-update:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "api-relayers-update:build:development"
        },
        "production": {
          "buildTarget": "api-relayers-update:build:production"
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/api/relayers/update/**/*.ts"]
      }
    }
  },
  "tags": []
}
