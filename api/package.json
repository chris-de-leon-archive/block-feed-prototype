{
  "name": "api",
  "version": "0.0.0",
  "license": "MIT",
  "engineStrict": true,
  "engines": {
    "node": "20.6.0"
  },
  "scripts": {
    "build": "nx run-many --targets build",
    "clean": "rm -rf .build .serverless dist",
    "setup": "docker compose -f ./tools/apps/docker/compose.yml up -d --build && npm run db:migrate -- --environment dev",
    "cleanup": "docker compose -f ./tools/apps/docker/compose.yml down",
    "api:start:dev": "NODE_ENV=development APP_ENV=dev serverless deploy && npm run api:id",
    "api:stop:dev": "NODE_ENV=development APP_ENV=dev serverless remove",
    "api:build:dev": "NODE_ENV=development APP_ENV=dev serverless package",
    "api:test": "bash ./tools/apps/api/test.sh",
    "api:id": "bash ./tools/apps/api/api-id.sh",
    "block:start": "bash ./tools/apps/block-gateway/start.sh",
    "block:build": "bash ./tools/apps/block-gateway/build.sh",
    "block:deploy": "bash ./tools/apps/block-gateway/deploy.sh",
    "block:destroy": "bash ./tools/apps/block-gateway/destroy.sh",
    "block:push": "bash ./tools/apps/block-gateway/push.sh",
    "block:test": "bash ./tools/apps/block-gateway/test.sh",
    "nx:app:create": "nx g @nx/node:application --directory apps --e2eTestRunner none --unitTestRunner none --framework none --bundler webpack",
    "nx:lib:create": "nx g @nx/node:lib --directory libs --buildable false --publishable false --strict true --testEnvironment node --unitTestRunner none",
    "nx:proj:delete": "nx g @nx/workspace:remove --project-name",
    "nx:proj:move": "nx g @nx/workspace:move",
    "openapi:gen:client": "bash ./tools/openapi/generate-code.sh ./libs/shared/testing/src/lib/core/openapi",
    "openapi:gen:docs": "ts-node ./tools/openapi/generate-docs.ts",
    "flow": "docker exec flow-emulator /root/.local/bin/flow",
    "redis-cli": "docker exec redis redis-cli",
    "db:migrate": "bash ./tools/db/migrate.sh",
    "db:info": "bash ./tools/db/info.sh",
    "auth0:token": "bash ./tools/auth0/get-token.sh"
  },
  "private": true,
  "dependencies": {
    "@aws-sdk/client-lambda": "3.423.0",
    "@onflow/fcl": "1.6.0",
    "@onflow/transport-grpc": "1.2.1",
    "@trpc/server": "10.38.5",
    "auth0": "4.0.1",
    "aws-lambda": "1.0.7",
    "bullmq": "4.12.0",
    "dotenv": "16.3.1",
    "drizzle-orm": "0.28.6",
    "drizzle-zod": "0.5.1",
    "pg": "8.11.3",
    "trpc-openapi": "1.2.0",
    "tslib": "2.3.0",
    "zod": "3.22.3"
  },
  "devDependencies": {
    "@aws-sdk/client-iam": "3.423.0",
    "@nx/js": "16.9.1",
    "@nx/node": "16.9.1",
    "@nx/webpack": "16.9.1",
    "@nx/workspace": "16.9.1",
    "@serverless/typescript": "3.30.1",
    "@types/auth0": "3.3.6",
    "@types/aws-lambda": "8.10.122",
    "@types/node": "20.6.2",
    "@types/pg": "8.10.3",
    "axios": "1.5.1",
    "drizzle-kit": "0.19.13",
    "nx": "16.9.1",
    "prettier": "^2.6.2",
    "serverless": "3.35.2",
    "serverless-localstack": "1.1.2",
    "serverless-webpack": "5.13.0",
    "ts-loader": "9.4.4",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.2.0",
    "tsconfig-paths-webpack-plugin": "4.1.0",
    "typescript": "5.2.2",
    "webpack-cli": "5.1.4",
    "webpack-node-externals": "3.0.0"
  }
}