{
  "name": "block-feed",
  "version": "1.0.0-alpha.1",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "fmt": "prettier . --write",
    "codegen": "graphql-codegen --require tsconfig-paths/register --config .graphqlrc.ts",
