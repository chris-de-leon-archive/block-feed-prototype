    "@tanstack/react-query": "5.28.6",
    "graphql": "16.8.1",
    "auth0": "4.3.1",
    "drizzle-orm": "0.29.4",
    "drizzle-zod": "0.5.1",
    "graphql-request": "6.1.0",
    "graphql-yoga": "5.2.0",
    "ioredis": "5.3.2",
    "mysql2": "3.9.1",
    "next": "14.1.0",
    "react": "18.2.0",
