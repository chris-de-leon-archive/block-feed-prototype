{
  "name": "@block-feed/dashboard",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "stripe:seed": "node --env-file=.env --import=tsx ./scripts/create-stripe-product-and-price.ts",
    "stripe:listen": "stripe listen --forward-to localhost:3001/api/graphql",
    "env:dev:show": "bws run --shell='bash' --project-id=\"${BWS_PROJECT_ID}\" -- 'echo \"${DEV_BACKEND}\"'",
    "env:dev:pull": "pnpm run --silent env:dev:show > .env",
    "schemagen": "node --import=tsx ./scripts/generate-gql-schema.ts",
    "codegen": "pnpm run schemagen && graphql-codegen --config .graphqlrc.ts",
    "test": "./node_modules/.bin/nptest",
    "dev": "next dev -p 3001",
    "clean": "rm -rf .turbo .next",
    "build": "pnpm run codegen && next build",
    "start": "next start",
    "lint": "eslint . --max-warnings 0"
  },
  "files": [
    "./public",
    "./.next"
  ],
  "dependencies": {
    "@block-feed/node-caching": "workspace:*",
    "@block-feed/node-db": "workspace:*",
    "@block-feed/node-providers-clerk": "workspace:*",
    "@block-feed/node-providers-mysql": "workspace:*",
    "@block-feed/node-providers-redis": "workspace:*",
    "@block-feed/node-providers-stripe": "workspace:*",
    "@block-feed/node-services-stripe-webhook-producer": "workspace:*",
    "@clerk/clerk-sdk-node": "5.0.49",
    "@clerk/nextjs": "5.7.2",
    "@escape.tech/graphql-armor-cost-limit": "2.3.0",
    "@escape.tech/graphql-armor-max-aliases": "2.6.0",
    "@escape.tech/graphql-armor-max-depth": "2.4.0",
    "@escape.tech/graphql-armor-max-directives": "2.3.0",
    "@escape.tech/graphql-armor-max-tokens": "2.5.0",
    "@graphql-typed-document-node/core": "3.2.0",
    "@pothos/core": "4.3.0",
    "@pothos/plugin-zod": "4.1.0",
    "@tanstack/react-query": "5.59.13",
    "drizzle-orm": "0.34.1",
    "graphql": "16.9.0",
    "graphql-yoga": "5.7.0",
    "jsonwebtoken": "9.0.2",
    "next": "14.2.15",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-icons": "5.3.0",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@block-feed/node-config-eslint": "workspace:*",
    "@block-feed/node-config-typescript": "workspace:*",
    "@block-feed/node-testutils": "workspace:*",
    "@graphql-codegen/cli": "5.0.3",
    "@graphql-codegen/client-preset": "4.4.0",
    "@types/eslint": "9.6.1",
    "@types/jsonwebtoken": "9.0.7",
    "@types/node": "22.7.5",
    "@types/react": "18.3.11",
    "@types/react-dom": "18.3.1",
    "autoprefixer": "10.4.20",
    "eslint": "9.12.0",
    "graphql-config": "5.1.3",
    "postcss": "8.4.47",
    "stripe": "17.2.0",
    "tailwindcss": "3.4.13",
    "typescript": "5.6.3"
  }
}
