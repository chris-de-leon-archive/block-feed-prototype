{
  "name": "@block-feed/dashboard",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "stripe:listen": "stripe listen --forward-to localhost:3001/api/graphql",
    "script": "TS_NODE_PROJECT=ts-node.tsconfig.json node --env-file=.env --require ts-node/register",
    "compose:up": "docker compose up -d --build",
    "compose:down": "docker compose down --remove-orphans",
    "compose": "pnpm compose:down && pnpm compose:up",
    "dev": "next dev -p 3001",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --max-warnings 0",
    "fmt": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "dependencies": {
    "@block-feed/api": "workspace:*",
    "@block-feed/shared": "workspace:*",
    "@block-feed/vendors": "workspace:*",
    "@clerk/nextjs": "4.29.12",
    "@escape.tech/graphql-armor-cost-limit": "2.1.0",
    "@escape.tech/graphql-armor-max-aliases": "2.3.0",
    "@escape.tech/graphql-armor-max-depth": "2.2.0",
    "@escape.tech/graphql-armor-max-directives": "2.1.0",
    "@escape.tech/graphql-armor-max-tokens": "2.3.0",
    "@graphql-typed-document-node/core": "3.2.0",
    "@pothos/core": "3.41.0",
    "@tanstack/react-query": "5.29.0",
    "graphql": "16.8.1",
    "graphql-request": "6.1.0",
    "graphql-yoga": "5.3.0",
    "next": "14.1.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "5.0.1",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@block-feed/eslint-config": "workspace:*",
    "@block-feed/typescript-config": "workspace:*",
    "@types/eslint": "8.56.5",
    "@types/node": "20.11.24",
    "@types/react": "18.2.61",
    "@types/react-dom": "18.2.19",
    "autoprefixer": "10.4.19",
    "eslint": "8.57.0",
    "postcss": "8.4.38",
    "prettier": "3.2.5",
    "prettier-plugin-tailwindcss": "0.5.13",
    "tailwindcss": "3.4.3",
    "ts-node": "10.9.2",
    "typescript": "5.3.3"
  }
}
