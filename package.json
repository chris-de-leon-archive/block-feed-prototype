{
  "name": "block-feed-prototype",
  "private": true,
  "scripts": {
    "turbo:app": "turbo gen workspace --type app",
    "turbo:pkg": "turbo gen workspace --type package",
    "deps:outdated": "pnpm outdated -r",
    "deps:up": "pnpm up --latest -r",
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "fmt": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "@block-feed/config-eslint": "workspace:*",
    "@block-feed/config-typescript": "workspace:*",
    "prettier": "3.3.1",
    "prettier-plugin-tailwindcss": "0.6.2",
    "turbo": "1.13.3"
  },
  "packageManager": "pnpm@8.9.0",
  "engines": {
    "node": ">=18"
  }
}
